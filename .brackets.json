{
    "sass.enabled": false,
    "sass.compiler": "libsass",
    "path": {
        "src/scss/main.scss": {
            "sass.enabled": true,
            "sass.options": {
                "includePaths": [
                    "src/scss/bower_components/bourbon/app/assets/stylesheets/"
                ],
                "outputDir": "../../css/",
                "imagePath": null,
                "sourceComments": false,
                "outputStyle": "expanded"
            },
            "linting.collapsed": false,
            "debug.showErrorsInStatusBar": true
        }
    }
}
